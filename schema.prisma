generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Deal {
  id        String   @id @default(cuid())
  title     String
  company   String
  value     Int
  stage     String
  closeDate DateTime
  createdAt DateTime @default(now())
}
